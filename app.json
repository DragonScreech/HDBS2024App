{
  "expo": {
    "name": "Houston Durga Bari",
    "slug": "ReactNativePuja",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "version": "8",
    "orientation": "portrait",
    "icon": "./assets/images/DurgaIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/HDBSSPLASH.png",
      "resizeMode": "contain",
      "backgroundColor": "#490000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.hbds.app",
      "versionCode": 8,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/HDBSLOGO.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "708e08bd-a382-4c14-a7d3-606cd2e3ce03"
      }
    }
  }
}